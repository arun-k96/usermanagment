<h5 class="fw-semibold text-headtext">Add Sequence</h5>
{{templateID}}
<div class="row border rounded ">
    <form action="/admin/addskills" method="POST">
        <div class="row mt-2">
            <div class="col-sm-12 col-md-4">
                <span class="fw-semibold text-headtext">Name</span>
                <span class="d-flex align-items-center"><i class="material-icons me-2">#1</i>
                    <input type="text" class="form-control my-2" name="skill1" aria-label="First name">
                </span> 
            </div>
            <div class="col-sm-12 col-md-8">
                <span class="fw-semibold text-headtext">Content</span>
                <textarea type="textarea" cols="2" rows="1" class="form-control my-2" placeholder="Description"
                    name="dskill1" aria-label="Description"></textarea>
            </div>
        </div>
        <div class="row mt-2">
            <div class="col-sm-12 col-md-4">
                <input type="text" class="form-control my-2" name="skill2" aria-label="First name">
            </div>
            <div class="col-sm-12 col-md-8">
                <textarea type="textarea" cols="2" rows="1" class="form-control my-2 ui-input-text" placeholder="Description"
                    name="dskill2" aria-label="Description"></textarea>
            </div>
        </div>
        <div class="row mt-2">
            <div class="col-sm-12 col-md-4">
                <input type="text" class="form-control my-2" name="skill3" aria-label="First name">
            </div>
            <div class="col-sm-12 col-md-8">
                <textarea type="textarea" cols="2" rows="1" class="form-control my-2 ui-input-text" placeholder="Description"
                    name="dskill3" aria-label="Description"></textarea>
            </div>
        </div>
        <div class="row mt-2">
            <div class="col-sm-12 col-md-4">
                <input type="text" class="form-control my-2" name="skill4" aria-label="First name">
            </div>
            <div class="col-sm-12 col-md-8">
                <textarea type="textarea" cols="2" rows="1" class="form-control my-2 ui-input-text" placeholder="Description"
                    name="dskill4" aria-label="Description"></textarea>
            </div>
        </div>
        <div class="row mt-2">
            <div class="col-sm-12 col-md-4">
                <input type="text" class="form-control my-2" name="skill5" aria-label="First name">
            </div>
            <div class="col-sm-12 col-md-8">
                <textarea type="textarea" cols="2" rows="1" class="form-control my-2 ui-input-text" placeholder="Description"
                    name="dskill5" aria-label="Description"></textarea>
            </div>
        </div>
        {{templateID}}
        <input type="hidden" name="templateID" value="{{templateID}}"/>
        <div>
            <button type="submit" class="btn btn-primary">Save</button>
        </div>
    </form>
</div>